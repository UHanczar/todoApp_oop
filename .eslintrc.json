{
  "extends": "airbnb",

  "parser": "babel-eslint",

  "env": {
      "browser": true
    },

  "rules": {
    "linebreak-style": [2, "windows"],
    "semi": [2, "always"],
    "no-debugger": 0,
    "no-console": 0,
    "new-cap": 0,
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "eol-last": 1,
    "comma-dangle": [0, "always"]
  }
}
